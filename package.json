{
  "name": "performance-management",
  "version": "0.0.1",
  "description": "performance management back-end app",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --fix",
    "tsc": "tsc",
    "build": "npm-run-all lint clean tsc",
    "start:dev": "npm-run-all lint tsc serve",
    "start": "nodemon --watch src -e ts,ejs --exec npm run start:dev",
    "start:prod": "pm2-runtime start ecosystem.config.js --env production",
    "serve": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "v14.16.0"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.13",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "pm2": "^4.5.0",
    "redis": "^3.1.2",
    "sequelize": "^6.3.5",
    "socket.io": "^4.1.0",
    "socket.io-redis": "^6.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^13.13.4",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@types/sequelize": "^4.28.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
